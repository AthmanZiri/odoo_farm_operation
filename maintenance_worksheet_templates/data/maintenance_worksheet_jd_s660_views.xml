                            </group>
                            <group string="GREASE">
                                <field name="x_grease_cleaning_fan_bearing"/>
                            </group>
                        </group>
                        <group>
                            <field name="x_comments"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

